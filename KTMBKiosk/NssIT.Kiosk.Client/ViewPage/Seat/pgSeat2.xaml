    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="rosSeatEnglish.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3.63*"/>
            <ColumnDefinition Width="1.17*"/>
            <ColumnDefinition Width="5.3*"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="85"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="160"/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3" Text="Pick your depart seat" Margin="10,0,0,0" Padding="5,3,5,3" Foreground="#FF777777" FontFamily="Roboto" FontSize="36" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" x:Name="TxtPageHeader"/>

        <Grid Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="4">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="0.8*"/>
                <ColumnDefinition Width="0.7*"/>
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0" Padding="15,5,15,5" Background="#FF999999">
                <TextBlock FontSize="21" FontWeight="Bold" Foreground="#FFFFFFFF"  VerticalAlignment="Center" Text="TRAIN 1" x:Name="TxtTrainNo"/>
            </Border>
            <Border Grid.Column="1" Padding="15,5,15,5" Background="#FFCCCCCC">
                <TextBlock FontSize="21" FontWeight="Normal" Foreground="#FF666666"  VerticalAlignment="Center" Text="ETS" x:Name="TxtService"/>
            </Border>
            <Border Grid.Column="2" Padding="15,5,15,5" Background="#FFEEEEEE">
                <TextBlock FontSize="21" FontWeight="Normal" Foreground="#FF666666"  VerticalAlignment="Center" Text="KUALA LUMPUR > GEMAS" TextWrapping="WrapWithOverflow" x:Name="TxtSubDepartDestination"/>
            </Border>
            <Border Grid.Column="3" Padding="15,5,15,5" Background="#FFDDDDDD">
                <TextBlock FontSize="21" FontWeight="Normal" Foreground="#FF666666"  VerticalAlignment="Center" Text="ETS Gold - EG 9321" x:Name="TxtServiceDesc"/>
            </Border>
            <Border Grid.Column="4" Padding="15,5,15,5" Background="#FFDDDDDD">
                <StackPanel Orientation="Horizontal" FlowDirection="RightToLeft">
                    <TextBlock FontSize="20" Foreground="#FF666666"  VerticalAlignment="Center" Text="2 Aug 2020 12:28" FlowDirection="LeftToRight" x:Name="TxtTicketDateTime"/>
                </StackPanel>
            </Border>
        </Grid>

