            <RowDefinition Height="15"/>
            
            <!-- 1) Header of Pax -->
            <RowDefinition Height="50"/>

            <!-- 2) Pax Selector -->
            <RowDefinition Height="Auto"/>

            <!-- 3) <Space> -->
            <RowDefinition Height="40"/>

            <!-- 4) Header of Travel Dates -->
            <RowDefinition Height="50"/>

            <!-- 5) Selected Depart & Return Date -->
            <RowDefinition Height="100" />

            <!-- 6) Calendar -->
            <RowDefinition Height="*" />

            <!-- 7) <Space> -->
            <RowDefinition Height="10" />

            <!-- 8) Search Button -->
            <RowDefinition Height="150" />

            <!-- 9) <Footer Space> -->
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <Grid.Resources>
            <ResourceDictionary>
                <sys:Double x:Key="circle_Day">75</sys:Double>
                <sys:Double x:Key="halfBlock_Day">67.142857142</sys:Double>
                <sys:Double x:Key="topBlock_Day">8</sys:Double>
                <sys:Double x:Key="bottomBlock_Day">8</sys:Double>
                <sys:Double x:Key="fontSize_Day">27</sys:Double>
