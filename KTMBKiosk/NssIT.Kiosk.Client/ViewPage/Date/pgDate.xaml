                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <TextBlock Text="{DynamicResource NEXTLabel}" Margin="0,5,0,0" Foreground="Black" HorizontalAlignment="Center" Width="70" TextAlignment="Center" Height="22" FontSize="16" />
            </StackPanel>

