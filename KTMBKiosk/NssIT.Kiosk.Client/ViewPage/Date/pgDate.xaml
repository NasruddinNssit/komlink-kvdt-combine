            <TextBlock Grid.Column="3" Grid.Row="0" Text="{DynamicResource RETURN_DATELabel}" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="22" Foreground="Black" />
            <TextBlock Grid.Column="1" Grid.Row="1" FontSize="24" Text="25 JULY 2020" HorizontalAlignment="Center" VerticalAlignment="Top" Foreground="#FF444444" FontWeight="Bold" x:Name="TxtDepartDate"/>
            <TextBlock Grid.Column="3" Grid.Row="1" FontSize="24" Text="31 JULY 2020" HorizontalAlignment="Center" VerticalAlignment="Top" Foreground="#FF444444" FontWeight="Bold" x:Name="TxtReturnDate"/>
        </Grid>
        
