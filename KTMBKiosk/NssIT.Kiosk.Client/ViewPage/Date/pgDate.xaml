                <ColumnDefinition Width="30"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="30"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="90" />
                <RowDefinition Height="10" />
                <RowDefinition Height="40" />

                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
            </Grid.RowDefinitions>

            <StackPanel Grid.Column="1" Grid.Row="0" Orientation="Vertical" MouseDown="PreviousMonth_MouseDown" Background="#00FFFFFF" x:Name="SkpPrevious">
                <TextBlock Text="" Margin="0,5,0,0"/>
                <Image Source="/NssIT.Kiosk.Client;component/Resources/icon-chevron-left-light.png" Height="45" Margin="35,0,35,0" Stretch="Fill" RenderTransformOrigin="0.5,0.5" >
                    <Image.RenderTransform>
                        <TransformGroup>
                            <RotateTransform Angle="0"/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <TextBlock Text="{DynamicResource PREVLabel}" Margin="0,5,0,0" Foreground="Black" HorizontalAlignment="Center"  Width="70" TextAlignment="Center" Height="22" FontSize="16" />
            </StackPanel>
            <TextBlock x:Name="TxtMonthYearTag" Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="5" FontSize="24" Text="JUNE 2020" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#FF444444" FontWeight="Bold" />
            <StackPanel Grid.Column="7" Grid.Row="0" Orientation="Vertical" MouseDown="NextMonth_MouseDown" Background="#00FFFFFF" x:Name="SkpNext">
