<Window x:Class="kvdt_kiosk.Views.Windows.ParcelWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kvdt_kiosk.Views.Windows"
        mc:Ignorable="d"
        Title="ParcelWindows" Height="991" Width="690" WindowStartupLocation="CenterScreen">
    <ScrollViewer>
        <Grid Background="#074481" Margin="0,8,0,8">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock Text="PARCEL" FontSize="40" FontWeight="bold" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
            <TextBlock Text="Date" Grid.Row="1" FontSize="35" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#D1D4D7
" Margin="0,8,0,8"></TextBlock>
            <Grid x:Name="GridParcel">

            </Grid>
        </Grid>
    </ScrollViewer>
  
</Window>
